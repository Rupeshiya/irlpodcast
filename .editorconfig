# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
insert_final_newline = true
charset = utf-8

#indentation
[*.{html,scss,js,yml,json}]
indent_style = space
indent_size = 2

[*.toml]
indent_style = space
indent_size = 4

[jenkinsfile]
indent_size = 4
